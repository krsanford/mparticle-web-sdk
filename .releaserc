{
    "branches": ["master"],
    "repositoryUrl": "https://github.com/rmi22186/rob-changelog-release-test",
    "tagFormat": "v${version}",
    "plugins": [
        ["@semantic-release/release-notes-generator", {
            "repository": "custom-repo",
            "owner": "custom-owner"
        }],
        ["@semantic-release/changelog", {"changelogFile": "CHANGELOG.md"}],
        ["@semantic-release/exec", {
            "prepareCmd": "./release.sh abcabc",
        }]
    ],
    "ci": false
}